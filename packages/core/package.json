{
    "name": "@hongkongkiwi/super-context-core",
    "version": "0.1.1",
    "description": "Core indexing engine for Super Context - AI-powered code search and semantic analysis",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./splitter": {
            "types": "./dist/splitter/index.d.ts",
            "import": "./dist/splitter/index.js",
            "default": "./dist/splitter/index.js"
        },
        "./embedding": {
            "types": "./dist/embedding/index.d.ts",
            "import": "./dist/embedding/index.js",
            "default": "./dist/embedding/index.js"
        },
        "./vectordb": {
            "types": "./dist/vectordb/index.d.ts",
            "import": "./dist/vectordb/index.js",
            "default": "./dist/vectordb/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.js",
            "default": "./dist/utils/index.js"
        },
        "./git": {
            "types": "./dist/git/index.d.ts",
            "import": "./dist/git/index.js",
            "default": "./dist/git/index.js"
        },
        "./search": {
            "types": "./dist/search/index.d.ts",
            "import": "./dist/search/index.js",
            "default": "./dist/search/index.js"
        },
        "./package.json": "./package.json"
    },
    "sideEffects": false,
    "scripts": {
        "build": "pnpm clean && pnpm build:tsc",
        "build:tsc": "tsc --build --force",
        "build:modern": "pnpm clean && tsup",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "typecheck": "tsc --noEmit",
        "validate": "pnpm lint && pnpm typecheck && publint && attw --pack",
        "prepublishOnly": "pnpm build && pnpm validate"
    },
    "dependencies": {
        "@aws-sdk/client-bedrock-runtime": "^3.689.0",
        "@aws-sdk/credential-providers": "^3.689.0",
        "@google-cloud/aiplatform": "^3.33.0",
        "@google/genai": "^1.9.0",
        "@huggingface/inference": "^2.8.0",
        "@qdrant/js-client-rest": "^1.12.0",
        "@zilliz/milvus2-sdk-node": "^2.5.10",
        "faiss-node": "^0.5.1",
        "@pinecone-database/pinecone": "^6.1.2",
        "pg": "^8.13.1",
        "weaviate-ts-client": "^2.2.0",
        "chromadb": "^1.9.1",
        "@upstash/vector": "^1.2.0",
        "firebase": "^10.0.0",
        "firebase-admin": "^12.0.0",
        "@aws-sdk/client-s3": "^3.0.0",
        "@aws-sdk/lib-storage": "^3.0.0",
        "@aws-sdk/credential-providers": "^3.0.0",
        "fs-extra": "^11.0.0",
        "glob": "^10.0.0",
        "langchain": "^0.3.27",
        "ollama": "^0.5.16",
        "openai": "^5.1.1",
        "tree-sitter": "^0.21.1",
        "tree-sitter-cpp": "^0.22.0",
        "tree-sitter-go": "^0.21.0",
        "tree-sitter-java": "^0.21.0",
        "tree-sitter-javascript": "^0.21.0",
        "tree-sitter-python": "^0.21.0",
        "tree-sitter-c-sharp": "^0.21.0",
        "tree-sitter-rust": "^0.21.0",
        "tree-sitter-typescript": "^0.21.0",
        "tree-sitter-scala": "^0.24.0",
        "typescript": "^5.0.0",
        "voyageai": "^0.0.4"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.0",
        "@types/node": "^20.0.0",
        "@types/pg": "^8.11.10"
    },
    "peerDependencies": {
        "typescript": ">=5.0.0"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/",
        "provenance": true
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "packageManager": "pnpm@>=10.0.0",
    "keywords": [
